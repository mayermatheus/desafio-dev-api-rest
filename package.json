{
  "name": "account.api",
  "version": "1.0.0",
  "description": "This is the backend for the Account's of digital bank.",
  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "cross-env NODE_ENV=dev ts-node-dev -r tsconfig-paths/register --inspect=4321 --respawn ./src/index.ts",
    "build": "rm -rf dist && tsc --build tsconfig.build.json && tscpaths -p tsconfig.build.json -s ./src -o ./dist",
    "lint": "npx eslint . --ext .js,.jsx,.ts,.tsx -f json -o ./target/lint/report.json | true",
    "lint:commit": "npx eslint . --ext .js,.jsx,.ts,.tsx --cache",
    "test": "cross-env NODE_ENV=test HTTP_PORT=3333 jest --coverage --passWithNoTests --detectOpenHandles --runInBand",
    "test:service": "cross-env NODE_ENV=test HTTP_PORT=3333 jest --coverage --passWithNoTests --detectOpenHandles --runInBand --testPathPattern=service\\.test\\.ts",
    "test:controller": "cross-env NODE_ENV=test HTTP_PORT=3333 jest --coverage --passWithNoTests --detectOpenHandles --runInBand --testPathPattern=controller\\.test\\.ts",
    "test:api": "cross-env NODE_ENV=test HTTP_PORT=3333 jest --coverage --passWithNoTests --detectOpenHandles --runInBand --testPathPattern=.spec\\.ts",
    "test:specific": "cross-env NODE_ENV=test HTTP_PORT=3333 jest --coverage --passWithNoTests --detectOpenHandles --runInBand",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "migration:generate": "cross-env NODE_ENV=dev yarn run typeorm migration:generate -n",
    "migration:run": "yarn run typeorm migration:run",
    "dockerbuild": "docker-compose up --build",
    "dockerup": "docker-compose up",
    "migration:run:dev": "cross-env NODE_ENV=dev yarn run typeorm migration:run"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mayermatheus/desafio-dev-api-rest"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mysql2": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "^0.2.34",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@jest-mock/express": "^1.4.2",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.5",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.12",
    "@types/faker": "^5.5.9",
    "@types/hapi__joi": "^17.1.6",
    "@types/jest": "^26.0.23",
    "@types/jsonwebtoken": "^8.5.2",
    "@types/node": "^15.12.2",
    "@types/source-map-support": "^0.5.3",
    "@types/supertest": "^2.0.11",
    "@types/swagger-jsdoc": "^6.0.0",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.3.1",
    "@types/moxios": "^0.4.11",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "cpf-cnpj-validator": "^1.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^7.24.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-promise": "^5.1.0",
    "faker": "^5.5.3",
    "jest": "^27.0.4",
    "jest-junit": "^12.2.0",
    "jest-mock-extended": "^2.0.1",
    "jest-sonar-reporter": "^2.0.0",
    "jsonwebtoken": "^8.5.1",
    "moxios": "^0.4.0",
    "prettier": "^2.3.0",
    "sonarqube-scanner": "^2.8.1",
    "supertest": "^6.1.5",
    "ts-jest": "^27.0.3",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.10.1",
    "tscpaths": "^0.0.9",
    "tslint": "^6.1.3",
    "typescript": "^4.3.2"
  },
  "jestSonar": {
    "reportPath": "target/test-results/jest",
    "reportFile": "TESTS-results-sonar.xml"
  }
}
